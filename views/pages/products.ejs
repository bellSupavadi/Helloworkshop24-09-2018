<% include ../partials/header %>

<table class="table">
    <thead>
        <tr>
            <td>ID</td>
            <td>Tittle</td>
            <td>Price</td>
            <td>Edit</td>
            <td>Delete</td>
           
        </tr>
    </thead>
    <tbody>
     <% products.forEach(function(product) { %>
        <tr>
            <td scope="row"><%= product.id %></td>
            <td><%= product.title %></td>
            <td><%= product.price %></td>
            <td><a name="" id="" class="btn btn-danger" href="products/<%= product.id %>" role="button">Edit</a></td>
            <td><a name="" id="" class="btn btn-danger" href="products_delete/<%= product.id %>" role="button">DELETE</a></td>
        </tr>
      <% }); %>
    </tbody>
</table>





  <% include ../partials/footer %>