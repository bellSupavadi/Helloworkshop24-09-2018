<% include ../partials/header %>
<table class = "table">
    <thead>
        <tr>
        <td>ID</td>
        <td>Title</td>
        <td>Price</td>
        <td>Edit</td>
        <td>Delete</td>
    </tr>
    </thead>
    <tbody>
        <% products.forEach(function(product){ %>
        <tr>
            <td scope ="row"><%= product.id %></td>
            <td><%= product.title</td>
            <td><%= product.price</td>
            
        </tr>
        <% }); %>
    </tbody>
</table>

<% include ../partials/footer %>