<% include ../partials/header %>
&nbsp;
<div class="jumbotron">
<h1 class="text-center">Report:Top 10 Customers by Sales</h1>
<table class="table">
    <thead>
        <tr>
            <th>Customers ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Total Sales</th>
        </tr>
    </thead>
    <tbody>
    <% users.forEach(function(user) { %>
        <tr>
            <td scope="row"><%= user.user_id %></td>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td>$<%= user.price %></td>
        </tr>
        <% }); %>
    </tbody>
</table></div>
 <% include ../partials/footer %>