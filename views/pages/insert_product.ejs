<% include ../partials/header %>

 <form action="/products/insert_product" method="post">
  <div class="jumbotron">
  <center> <h1>NEW PRODUCT</h1></center>
   <br>


  <div class="form-group">
    <label for="title">Product Name</label>
    <input type="text"
      class="form-control" name="title" id="" aria-describedby="helpId" placeholder=""value="">
    <!-- <small id="helpId" class="form-text text-muted">Title</small> -->
  </div>

  <div class="form-group">
    <label for="Price">Price</label>
    <input type="number"
      class="form-control" name="price" id="" aria-describedby="helpId" placeholder=""value="">
    <!-- <small id="helpId" class="form-text text-muted">Price</small> -->
    </div>

    <div class="form-group">
      <label for="">Create At</label>
      <input type="text"
      class="form-control" value="<%= time %>" id="" aria-describedby="helpId" placeholder="" disabled>
      <input type="hidden" value="<%= time %>"id="custId" name="time" >
      <!-- <small id="helpId" class="form-text text-muted">create at</small> -->
    </div>
    <br>
   <center> <button type="submit" class="btn btn-success">Save</button>
    <a name="" id="" class="btn btn-light" href="/products" role="button">Cancel</a></center>
 </div>
</from> 
  <% include ../partials/footer %>